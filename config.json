[
    {
        "brews": [
            "bash-completion",
            "vim", 
            "less",
            "git", 
            "jq"
        ], 
        "symlinks": {
            "~/.bash_additions": "./bash/bash_additions", 
            "~/.bash_mac": "./bash/bash_mac", 
            "~/.bash_aliases": "./bash/bash_aliases", 
            "~/.bash_logout": "./bash/bash_logout", 
            "~/.bashrc": "./bash/bashrc", 
            "~/.gitignore": "./git/gitignore", 
            "~/.gitconfig": "./git/gitconfig", 
            "~/.vim": "./vim",
            "~/.bash_utils": "./bash/utils",
            "~/.editrc": "./bash/editrc",
            "~/Library/Preferences/com.googlecode.iterm2.plist": "./iterm/iterm2.plist"
        },
        "copy": {
            "~/.vim_undo": "./vim/vim_undo"
        }
    }, 
    {
        "symlinks": {
            "~/.bash_profile": "~/.bashrc", 
            "~/.vimrc": "~/.vim/vimrc",
            "~/.ipython/profile_default/ipython_config.py": "./ipython/ipython_config.py"
        },
        "commands": [
            "lesskey ./less/lesskey",
            "printf '\n# git branch alias branch autocompletion\n__git_complete gco _git_branch\n__git_complete gco _git_branch' >> NEED_RIGHT_PATH_TO_COMPLETION_FILE"
        ]
    }
]
